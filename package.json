        "solid-devtools": "0.27.7",
        "tailwindcss": "3.3.3",
        "ts-loader": "9.5.0",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.2.2",
        "vite": "4.4.11",
        "vite-plugin-solid": "2.7.0"
