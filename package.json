{
    "name": "twitch-bot",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "env-cmd -f .env.dev ts-node src/index.mts",
        "build": "rollup --config",
        "start": "env-cmd -f .env node ./bundle.mjs",
        "lint": "eslint ./",
        "typecheck": "tsc --noEmit",
        "prettier:check": "prettier  --check .",
        "prettier:write": "prettier --write .",
        "prepare": "husky install"
    },
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanesik/twitch-bot.git"
    },
    "author": "Ivan Eleskin",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/ivanesik/twitch-bot/issues"
    },
    "homepage": "https://github.com/ivanesik/twitch-bot#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-typescript": "11.0.0",
        "@types/lodash-es": "4.17.6",
        "@types/node": "18.14.1",
        "@types/ws": "8.5.4",
        "@typescript-eslint/eslint-plugin": "5.53.0",
        "@typescript-eslint/parser": "5.53.0",
        "eslint": "8.34.0",
        "eslint-config-prettier": "8.6.0",
        "husky": "8.0.3",
        "prettier": "2.8.4",
        "rollup": "3.17.2",
        "ts-node": "10.9.1",
        "tslib": "2.5.0",
        "typescript": "4.9.5"
    },
    "dependencies": {
        "env-cmd": "10.1.0",
        "lodash-es": "4.17.21",
        "winston": "3.8.2",
        "winston-daily-rotate-file": "4.7.1",
        "ws": "8.12.1"
    }
}
