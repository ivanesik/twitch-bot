{
    "name": "twitch-bot",
    "version": "0.0.1",
    "author": "ivanesik",
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
