        "nest-winston": "1.9.4",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.8.1",
        "solid-js": "1.7.12",
        "winston": "3.11.0",
        "winston-daily-rotate-file": "4.7.1"
    },
    "devDependencies": {
        "@nestjs/cli": "10.1.18",
