{
    "name": "twitch-bot",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "env-cmd -f .env.dev ts-node src/index.mts",
        "build": "rollup --config",
        "start": "env-cmd -f .env node ./bundle.mjs",
        "lint": "eslint ./",
        "typecheck": "tsc --noEmit",
        "prettier:check": "prettier  --check .",
        "prettier:write": "prettier --write .",
        "prepare": "husky install"
    },
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanesik/twitch-bot.git"
    },
    "author": "Ivan Eleskin",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/ivanesik/twitch-bot/issues"
    },
    "homepage": "https://github.com/ivanesik/twitch-bot#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "25.0.3",
        "@rollup/plugin-json": "6.0.0",
        "@rollup/plugin-node-resolve": "15.1.0",
        "@rollup/plugin-typescript": "11.1.2",
        "@types/lodash-es": "4.17.8",
        "@types/node": "18.14.1",
        "@types/ws": "8.5.5",
        "@typescript-eslint/eslint-plugin": "6.2.0",
        "@typescript-eslint/parser": "6.2.0",
        "eslint": "8.46.0",
        "eslint-config-prettier": "8.9.0",
        "husky": "8.0.3",
        "lint-staged": "13.2.3",
        "prettier": "3.0.0",
        "rollup": "3.27.0",
        "ts-node": "10.9.1",
        "tslib": "2.6.1",
        "typescript": "5.1.6"
    },
    "dependencies": {
        "env-cmd": "10.1.0",
        "lodash-es": "4.17.21",
        "winston": "3.10.0",
        "winston-daily-rotate-file": "4.7.1",
        "ws": "8.13.0",
        "zod": "3.21.4"
    }
}
